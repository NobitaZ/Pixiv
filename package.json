    "dotenv": "^10.0.0",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "getmac": "^5.20.0",
    "mongoose": "^6.0.3",
    "node-notifier": "^10.0.0",
    "nodemon": "^2.0.12",
    "public-ip": "^4.0.4",
    "puppeteer": "^10.2.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-adblocker": "^2.11.11",
    "puppeteer-extra-plugin-recaptcha": "^3.4.0",
