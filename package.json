  "description": "",
  "main": "main.js",
  "build": {
    "productName": "Pixiv Crawl Tool",
    "appId": "pixiv-crawl-tool",
    "win": {
      "publish": [
        "github"
      ],
      "target": "nsis",
      "icon": "icon.jpg"
    },
    "extraResources": [
      {
        "from": "data",
        "to": "data"
